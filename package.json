{
  "name": "bun-plugin-vue",
  "description": "Bun plugin for loading Vue files",
  "version": "0.0.1",
  "type": "module",
  "author": "pandawabs",
  "license": "MIT",
  "homepage": "https://github.com/pandawabs/bun-plugin-vue#readme",
  "bugs": "https://github.com/pandawabs/bun-plugin-vue/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pandawabs/bun-plugin-vue.git"
  },
  "keywords": [
    "bun",
    "plugin",
    "vue",
    "vue3",
    "loader",
    "loading"
  ],
  "files": [
      "/dist"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun",
    "dev": "pnpm run build -- --watch"
  },
  "dependencies": {
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@tsconfig/bun": "^1.0.7",
    "vue-tsc": ">=2.0.0",
    "typescript": "^5.0.0"
  }
}
